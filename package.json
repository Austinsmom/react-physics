{
  "name": "react-physics",
  "description": "Set of universal react components implementing a physic engine",
  "version": "0.1.1",
  "main": "index.js",
  "dependencies": {
    "babel-runtime": "^5.8.25",
    "p2": "^0.7.0",
    "react": "0.x"
  },
  "scripts": {
    "prebuild": "rm -rf build && mkdir build",
    "build": "babel src/ --out-dir build/",
    "eslint": "eslint src/",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mathieudutour/react-physics"
  },
  "keywords": [
    "react",
    "components",
    "universal",
    "physic",
    "engine",
    "p2"
  ],
  "author": "Mathieu Dutour",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mathieudutour/react-physics/issues"
  },
  "homepage": "https://github.com/mathieudutour/react-physics",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.1",
    "eslint": "^1.1.0",
    "eslint-config-defaults": "^4.0.1",
    "eslint-plugin-react": "^3.3.2",
    "ghooks": "^0.3.2"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run eslint && npm run test"
    }
  },
  "babel": {
    "stage": 0,
    "optional": [
      "runtime"
    ]
  }
